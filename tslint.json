{
  "defaultSeverity": "error",
  "extends": [
      "tslint-react"
  ],
  "jsRules": {
  },
  "rules": {
      "member-access": false,
      "ordered-imports": false,
      "quotemark": [true, "single", "avoid-escape", "avoid-template", "jsx-double"],
      "no-console": true,
      "semicolon": [true, "always"],
      "jsx-no-lambda": true,
      "jsx-no-multiline-js": false,
      "jsx-boolean-value": false
  },
  "rulesDirectory": [
  ],
  "linterOptions": {
      "exclude": [
          "config/**/*.js",
          "node_modules/**/*.ts"
     ]
 }
}